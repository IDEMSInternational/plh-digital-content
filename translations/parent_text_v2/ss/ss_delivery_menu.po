msgid "@(text_slice(results.message_options,5))."
msgstr "@(text_slice(results.message_options,5))."

#. type=text
#. Strings like @fields.xxx and @results.yyy should not be translated. 
msgctxt "text"
msgid "This course has @fields.n_goals_prog goals, which one do you want to work on first?@results.message_options"
msgstr "Loluhlelo lwekufundza luna @fields.n_goals_prog wemigomo, ngumuphi lofuna kucala ngawo kuwusebenta?@results.message_options"

#. type=text
msgctxt "text"
msgid "Sorry, I don't understand what you mean."
msgstr "Ncesi, angicondzisisi kutsi utsini."

#. type=text
msgctxt "text"
msgid "You have seen all the goals "
msgstr "Sewuyibonile yonkhe imigomo "

#. type=text
msgctxt "text"
msgid "You are so close to completing the course! You only have one goal left,"
msgstr "Sekusele kancane nje kutsi ucedze loluhlelo lwekufundza! Sewusalelwe ngumgomo munye,"

#. type=text
msgctxt "text"
msgid "That's okay."
msgstr "Kukahle loko."

#. type=text
msgctxt "text"
msgid "What do you want to do next?"
msgstr "Ufuna kwenta ini kulokulandzelako?"

