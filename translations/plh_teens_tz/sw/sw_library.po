#. type=data_list
msgctxt "data_list"
msgid "I need help with:"
msgstr "Ninahitaji msaada na:"

#. type=data_list
msgctxt "data_list"
msgid "Essential Tools"
msgstr "Nyenzo Muhimu"

#. type=data_list
