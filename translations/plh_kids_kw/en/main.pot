#. type=data_list
#. The following strings should not be translated: 
#.      @data.characters.f3_daughter_7.name
#.      @data.characters.f3_son_4.name
msgctxt "data_list"
msgid "@data.characters.f3_daughter_7.name did not want to play with @data.characters.f3_son_4.name"
msgstr ""

#. type=data_list
#. The following strings should not be translated: 
#.      @data.characters.f3_son_4.namel
#.      @data.characters.f3_daughter_7
msgctxt "data_list"
msgid "@data.characters.f3_son_4.namel and @data.characters.f3_daughter_7 both wanted to play, but they needed to find a way to share the magnet tiles. The real problem was that there weren‚Äôt enough tiles for both to build separate houses."
msgstr ""

#. type=data_list
