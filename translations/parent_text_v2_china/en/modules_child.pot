msgstr ""

#. type=text
msgctxt "text"
msgid "1️⃣ LEARN"
msgstr ""

#. type=text
msgctxt "text"
msgid "2️⃣ PROTECT"
msgstr ""

#. type=text
msgctxt "text"
msgid "3️⃣ BUILD HABITS"
msgstr ""

#. type=text
msgctxt "text"
msgid "4️⃣ BUILD TRUST"
msgstr ""

#. type=text
