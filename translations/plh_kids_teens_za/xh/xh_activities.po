msgctxt "data_list"
msgid "Quick"
msgstr "Ngokukhawuleza"

#. type=data_list
msgctxt "data_list"
msgid "Group"
msgstr "Iqela"

#. type=data_list
msgctxt "data_list"
msgid "Active"
msgstr "Phithizela"

