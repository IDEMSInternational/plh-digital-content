#. type=data_list
msgctxt "data_list"
msgid "More tips"
msgstr "Ezinye ingcebiso"

#. type=data_list
msgctxt "data_list"
msgid "Other"
msgstr "Enye indlela"

#. type=data_list
msgctxt "data_list"
msgid "Yes"
msgstr "Ewe"

#. type=data_list
msgctxt "data_list"
msgid "That's great! Parents who practise praise at home get better results!"
msgstr "Kuhle kakhulu! Abazali abaziqhelanisa nokuncoma ekhaya bafumana iziphumo ezingcono!"

#. type=data_list
msgctxt "data_list"
