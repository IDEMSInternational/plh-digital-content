msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Disclaimer"
msgstr ""

#. type=data_list
#. The following strings should not be translated: 
#.      @global.app_name
#.      @global.app_name
#.      @global.app_name
#.      @global.app_name
#.      @global.app_name,
#.      @global.app_name
msgctxt "data_list"
msgid ""
"Please be aware that the @global.app_name team exclude responsibility for the following:\n"
"\n"
