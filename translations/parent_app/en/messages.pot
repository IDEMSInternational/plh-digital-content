#. type=data_list
msgctxt "data_list"
msgid "Everyday activities"
msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Have fun AND learn through everyday activities."
msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Learning at home"
msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Help grow your teen’s skills for school and life."
msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Fun and games"
msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Play games to strengthen your relationship and your teen’s skills."
msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Chat together"
msgstr ""

#. type=data_list
msgctxt "data_list"
msgid "Talk with each other to know each other better."
msgstr ""

#. type=data_list
