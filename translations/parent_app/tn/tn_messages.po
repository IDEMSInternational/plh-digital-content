msgid "@global.weekly_workshop day"
msgstr "letsatsi la @global.weekly_workshop"

#. type=template
msgctxt "template"
msgid "I do the workshops..."
msgstr "Ke dira di-workshop..."

#. type=template
msgctxt "template"
msgid "My group's name"
msgstr "Leina la setlhopha sa me"

#. type=template
