msgctxt "template"
msgid "CALL"
msgstr "FOWUNANI"

#. type=template
msgctxt "template"
msgid "TEXT"
msgstr "THUMELA UMYALEZO"

#. type=template
msgctxt "template"
msgid "SHARE"
msgstr "YABELANANI"

#. type=template
